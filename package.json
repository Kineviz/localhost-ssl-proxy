{
  "name": "localhost-ssl-proxy",
  "version": "1.0.0",
  "author": "Sean Li <sean@kineviz.com>",
  "description": "Let proxy to https localhost simply.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kineviz/localhost-ssl-proxy.git"
  },
  "license": "MIT",
  "bin": {
    "localhost-ssl-proxy": "bin/localhost-ssl-proxy",
    "ssl-proxy": "bin/localhost-ssl-proxy"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "dev": "nodemon --exec \"node ./bin/localhost-ssl-proxy -p 8008\" --watch ./bin/localhost-ssl-proxy --watch ./lib  ",
    "test": "yarn unlink || yarn link && yarn link && localhost-ssl-proxy -p 8008",
    "release": "npm  publish --registry=https://registry.npmjs.org"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "devcert": "^1.2.0",
    "http-proxy": "^1.18.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.13"
  }
}
