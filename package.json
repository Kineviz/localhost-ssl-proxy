{
  "name": "localhost-ssl-proxy",
  "version": "1.0.6",
  "author": "Sean Li <sean@kineviz.com>",
  "description": "Let proxy to https localhost simply.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kineviz/localhost-ssl-proxy.git"
  },
  "license": "MIT",
  "bin": {
    "localhost-ssl-proxy": "bin/localhost-ssl-proxy",
    "ssl-proxy": "bin/localhost-ssl-proxy"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "dev": "nodemon --exec \"node ./bin/localhost-ssl-proxy -p 8008 --useCORS false \" --watch ./bin/localhost-ssl-proxy --watch ./lib  ",
    "test": "yarn unlink localhost-ssl-proxy || yarn link && yarn link && localhost-ssl-proxy -p 8008",
    "release": "npm  publish --registry=https://registry.npmjs.org",
    "docker_build": "docker build -t kineviz/localhost-ssl-proxy:latest ./  --no-cache",
    "docker_push": "docker push kineviz/localhost-ssl-proxy:latest",
    "docker_test": "cd ./docker-compose-for-nebula-graphdb && docker-compose up -d"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "cors-anywhere": "github:lixuanxian/cors-anywhere#master",
    "devcert": "^1.2.0",
    "express": "^4.18.1",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^2.0.6",
    "serve-index": "^1.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.13"
  }
}
